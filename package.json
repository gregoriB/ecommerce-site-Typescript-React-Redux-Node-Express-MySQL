{
    "name": "ecommerce-site",
    "version": "0.1.0",
    "description": "Ecommerce site with shopping cart.  Uses React for the front-end and Node/Express for the back-end.",
    "main": "server.ts",
    "dependencies": {
        "@types/express": "^4.17.0",
        "concurrently": "^4.1.1",
        "ecommerce-react-redux": "file:client",
        "express": "^4.17.1"
    },
    "devDependencies": {},
    "scripts": {
        "server": "ts-node server.ts",
        "client": "npm start --prefix client",
        "react-build": "cd ./client && npm run build",
        "build": "concurrently \"npm build ./\" \"npm run react-build\"",
        "start": "concurrently \"npm run server\" \"npm run client\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gregoriB/ecommerce-site-typescript-react-redux.git"
    },
    "keywords": [
        "ecommerce",
        "shopping",
        "cart",
        "node",
        "express",
        "react",
        "redux",
        "typescript"
    ],
    "author": "Brandon Gregori",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/gregoriB/ecommerce-site-typescript-react-redux/issues"
    },
    "homepage": "https://github.com/gregoriB/ecommerce-site-typescript-react-redux#readme"
}
