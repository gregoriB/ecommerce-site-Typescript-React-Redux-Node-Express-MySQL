    "main": "server.ts",
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.21",
        "@fortawesome/free-solid-svg-icons": "^5.10.1",
        "@fortawesome/react-fontawesome": "^0.1.4",
        "@types/react-transition-group": "^4.2.2",
        "bootstrap": "^4.3.1",
        "cors": "^2.8.5",
        "dotenv": "^8.0.0",
        "ecommerce-react-redux": "file:client",
        "express": "^4.17.1",
        "pg": "^7.12.1",
        "react": "^16.8.6",
        "react-bootstrap": "^1.0.0-beta.12",
        "react-dom": "^16.8.6",
        "react-magnifier": "^3.0.1",
        "react-redux": "^7.1.0",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "react-scripts": "3.0.1",
        "react-transition-group": "^4.2.1",
        "styled-components": "^4.3.2",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.5",
        "@types/express": "^4.17.0",
        "@types/jest": "24.0.15",
        "@types/node": "12.6.8",
        "@types/react": "16.8.23",
        "@types/react-dom": "16.8.5",
        "@types/react-redux": "^7.1.1",
        "@types/react-router": "^5.0.3",
        "@types/react-router-dom": "^4.3.4",
        "@types/styled-components": "^4.1.18",
        "@types/uuid": "^3.4.5",
        "concurrently": "^4.1.1",
        "nodemon": "^1.19.1",
        "ts-node": "^8.3.0",
        "typescript": "3.5.3"
    },
    "scripts": {
        "build": "concurrently \"tsc -p .\" \"npm run react-build\"",
        "client": "npm start --prefix client",
        "dev": "nodemon server.ts",
        "react-build": "cd ./client && react-scripts build",
        "server": "node-ts server.ts",
        "devStart": "concurrently \"npm run dev\" \"npm run client\"",
        "start": "node-ts server.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gregoriB/super-meter-arcade-ecommerce-site.git"
